tHandler,\n    emptyCart: emptyCartHandler\n  };\n\n  return (\n    <CartContext.Provider value={cartContext}>\n      {props.children}\n    </CartContext.Provider>\n  );\n};\n\nexport default CartProvider;\n","/home/harshraj/Desktop/Market-App/src/store/cart-context.js",[],"/home/harshraj/Desktop/Market-App/src/components/Cart/CartItem.js",[],"/home/harshraj/Desktop/Market-App/src/components/Meals/AvailableMeals.js",[],"/home/harshraj/Desktop/Market-App/src/components/Cart/Checkout.js",["104"],"/home/harshraj/Desktop/Market-App/src/components/Meals/MealItem/MealItemDetail.js",["105","106","107","108","109","110","111"],"/home/harshraj/Desktop/Market-App/src/components/UI/LoadingSpinner.js",[],"/home/harshraj/Desktop/Market-App/src/redux-store/index.js",[],"/home/harshraj/Desktop/Market-App/src/components/Meals/MealItem/MealItemDetailForm.js",[],{"ruleId":"112","severity":1,"message":"113","line":17,"column":9,"nodeType":"114","messageId":"115","endLine":17,"endColumn":16},{"ruleId":"116","replacedBy":"117"},{"ruleId":"118","replacedBy":"119"},{"ruleId":"112","severity":1,"message":"120","line":1,"column":10,"nodeType":"114","messageId":"115","endLine":1,"endColumn":20},{"ruleId":"112","severity":1,"message":"121","line":5,"column":8,"nodeType":"114","messageId":"115","endLine":5,"endColumn":19},{"ruleId":"112","severity":1,"message":"122","line":17,"column":9,"nodeType":"114","messageId":"115","endLine":17,"endColumn":14},{"ruleId":"112","severity":1,"message":"123","line":18,"column":9,"nodeType":"114","messageId":"115","endLine":18,"endColumn":20},{"ruleId":"112","severity":1,"message":"124","line":2,"column":10,"nodeType":"114","messageId":"115","endLine":2,"endColumn":14},{"ruleId":"112","severity":1,"message":"125","line":25,"column":9,"nodeType":"114","messageId":"115","endLine":25,"endColumn":23},{"ruleId":"112","severity":1,"message":"126","line":2,"column":10,"nodeType":"114","messageId":"115","endLine":2,"endColumn":13},{"ruleId":"112","severity":1,"messag